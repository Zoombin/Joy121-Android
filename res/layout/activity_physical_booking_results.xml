<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WHITE"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/bg"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/layout_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/title_bg" >

            <ImageView
                android:id="@+id/iv_ret"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:scaleType="fitXY"
                android:src="@drawable/arrow_left" />

            <TextView
                android:id="@+id/tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="预约结果"
                android:textColor="@color/WHITE" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/WHITE"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dip"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/iv_bookingResultsDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/entry_date" />

                <TextView
                    android:id="@+id/tv_bookingResultsDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/bookingDate"
                    android:textColor="@color/BLACK" />

                <EditText
                    android:id="@+id/et_bookingResultsDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:inputType="none"
                    android:singleLine="true"
                    android:background="@null"
                    android:textColor="@color/BLACK" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dip"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/iv_bookingResultsPlace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/entry_nowaddress" />

                <TextView
                    android:id="@+id/tv_bookingResultsPlace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/bookingPlace"
                    android:textColor="@color/BLACK" />

                <EditText
                    android:id="@+id/et_bookingResultsPlace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:inputType="none"
                    android:singleLine="true"
                    android:background="@null"
                    android:textColor="@color/BLACK" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dip"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/iv_trafficRoutes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/entry_birthplace"/>

                <TextView
                    android:id="@+id/tv_trafficRoutes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/trafficRoutes"
                    android:textColor="@color/BLACK" />

                <EditText
                    android:id="@+id/et_trafficRoutes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                     android:layout_marginRight="15dip"
                     android:layout_marginTop="10dip"
                    android:focusable="false"
                    android:inputType="textMultiLine"
                    android:background="@null"
                    android:textColor="@color/BLACK" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dip"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/iv_medicalInformation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/entry_checkreporting" />

                <TextView
                    android:id="@+id/tv_medicalInformation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/medicalInformation"
                    android:textColor="@color/BLACK" />

                <EditText
                    android:id="@+id/et_medicalInformation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dip"
                    android:layout_marginTop="10dip"
                     android:inputType="textMultiLine"
                     android:focusable="false"
                     android:background="@null"
                    android:textColor="@color/BLACK" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>